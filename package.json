{
  "name": "@summit-electric-supply/ember-peak-spinner",
  "version": "1.1.1",
  "description": "Simple, easily customizable spinner UI for your Ember application",
  "keywords": [
    "ember-addon"
  ],
  "bugs": "https://github.com/summit-electric-supply/ember-peak-spinner/issues",
  "repository": "https://github.com/summit-electric-supply/ember-peak-spinner",
  "license": "MIT",
  "author": "Steve Jabour <steve@jabour.me> (https://github.com/atsjj)",
  "contributors": [
    "Andrei Hristian <hristian.andrei@gmail.com> (https://github.com/ahristian)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:test": "ember build --environment=test",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:dependency": "ember dependency-lint",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember exam --query=nolint --split=2 --parallel=1",
    "test:ember-compatibility": "ember try:each",
    "ci:ember-compatibility": "ember try:one"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.2",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-typescript": "^5.1.1"
  },
  "devDependencies": {
    "ember-concurrency": "^1.2.1",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-data": "^3.21.0",
    "ember-moment": "^8.0.0",
    "@embroider/test-setup": "^2.0.0",
    "@types/ember-qunit": "^5.0.1",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^4.0.2",
    "@types/ember__component": "^4.0.10",
    "@types/ember__controller": "^4.0.1",
    "@types/ember__debug": "^4.0.1",
    "@types/ember__engine": "^4.0.2",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.4",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.10",
    "@types/ember__runloop": "^4.0.1",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-github-pages": "^0.2.2",
    "ember-exam": "^5.0.1",
    "ember-page-title": "^6.2.1",
    "ember-prism": "^0.8.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "release-it": "^15.4.1",
    "release-it-lerna-changelog": "^5.0.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.4.3",
    "webpack": "5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "auto-changelog": {
    "commitLimit": false,
    "issueUrl": "https://github.com/summit-electric-supply/ember-peak-spinner/issues/{id}",
    "package": "package.json",
    "releaseSummary": true,
    "template": "https://gist.githubusercontent.com/atsjj/de6998d6750a8b32778fa3380da24301/raw/66a23990c3324f03aa756e45fed70d46389c7eab/normal.hbs"
  },
  "release-it": {
    "git": {
      "changelog": "npx auto-changelog --stdout --template https://gist.githubusercontent.com/atsjj/de6998d6750a8b32778fa3380da24301/raw/66a23990c3324f03aa756e45fed70d46389c7eab/compact.hbs",
      "requireCleanWorkingDir": true,
      "requireUpstream": true,
      "requireCommits": false,
      "addUntrackedFiles": false,
      "commit": true,
      "commitMessage": "${version}",
      "commitArgs": "--gpg-sign",
      "tag": true,
      "tagName": "${version}",
      "tagAnnotation": "${version}",
      "tagArgs": "--sign",
      "push": true,
      "pushArgs": "--follow-tags",
      "pushRepo": "origin"
    },
    "npm": {
      "publish": true
    }
  }
}
